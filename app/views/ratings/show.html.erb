<p id="notice"><%= notice %></p>

  <h1><%= @rating.title %></h1>

<blockquote>
  <p><%= @rating.comments %></p>
  <small><%= @rating.name %> - <%= @rating.stars %> stars</small>
</blockquote>


<div class="actions">
  <%= link_to 'Edit', edit_rating_path(@rating), :class => 'btn' %>
  <%= link_to 'Back', ratings_path, :class => 'btn' %>
</div>
